{
  "api-key": {
    "name": "api-key",
    "type": "api-key",
    "disabled": true
  },
  "local-userpass": {
    "name": "local-userpass",
    "type": "local-userpass",
    "disabled": false,
    "config": {
      "autoConfirm": true,
      "resetFunctionName": "resetPasswordHandler",
      "resetPasswordSubject": "reset",
      "resetPasswordUrl": "http://localhost/reset",
      "runConfirmationFunction": false,
      "runResetFunction": true,
      "mailConfig": {
        "from": "no-reply@example.com",
        "subject": "Password Reset",
        "mailToken": "ENV.MAIL_TOKEN"
      }
    }
  },
  "custom-function": {
    "name": "custom-function",
    "type": "custom-function",
    "disabled": true,
    "config": {
      "authFunctionName": ""
    }
  },
  "anon-user": {
    "name": "anon-user",
    "type": "anon-user",
    "disabled": false
  }
}
