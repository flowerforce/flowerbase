{
  "api-key": {
    "name": "api-key",
    "type": "api-key",
    "disabled": true
  },
  "local-userpass": {
    "name": "local-userpass",
    "type": "local-userpass",
    "disabled": false,
    "config": {
      "autoConfirm": true,
      "resetPasswordSubject": "reset",
      "resetPasswordUrl": "http://localhost/reset",
      "runConfirmationFunction": false,
      "runResetFunction": false,
      "mailConfig": {
        "from": "no-reply@example.com",
        "subject": "Password Reset",
        "mailToken": "ENV.MAIL_TOKEN"
      }
    }
  }
}
